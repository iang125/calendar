<h1>Listing responses</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Atending</th>
      <th>Arivle time</th>
      <th>Deparcher time</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @responses.each do |response| %>
      <tr>
        <td><%= response.atending %></td>
        <td><%= response.arivle_time %></td>
        <td><%= response.deparcher_time %></td>
        <td><%= response.user_id %></td>
        <td><%= link_to 'Show', response %></td>
        <td><%= link_to 'Edit', edit_response_path(response) %></td>
        <td><%= link_to 'Destroy', response, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response', new_response_path %>
